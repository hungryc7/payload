  },
  "dependencies": {
    "@libsql/client": "^0.3.1",
    "drizzle-kit": "0.19.13-e99bac1",
    "drizzle-orm": "0.28.5",
    "pg": "8.11.3",
    "prompts": "2.4.2",
    "to-snake-case": "1.0.0"
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "workspace:*",
    "@types/pg": "8.10.2",
    "@types/to-snake-case": "1.0.0",
    "better-sqlite3": "^8.5.0",
    "payload": "workspace:*"
  },
  "peerDependencies": {
    "better-sqlite3": "^8.5.0"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts"
  }
}
